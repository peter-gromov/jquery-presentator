fontL = 'Cuprum', sans-serif
fontSI = 'segoe_ui_light', 'Verdana', 'Arial', 'sans-serif'
vendor(prop, args)
	-webkit-{prop} args
	-moz-{prop} args
	-ms-{prop} args
	-o-{prop} args
	{prop} args
animation()
	vendor('animation', arguments)
border-radius()
	vendor('border-radius', arguments)
box-shadow()
	vendor('box-shadow', arguments)
box-sizing()
	vendor('box-sizing', arguments)
transition()
	vendor('transition', arguments)
transform()
	vendor('transform', arguments)
transform-origin()
	vendor('transform-origin', arguments)
user-select()
	vendor('user-select', arguments)
background-size(v)
	vendor('background-size', arguments)
background-linear-gradient(startPoint, startColor, startInterval, endColor, endInterval, deprecatedWebkitStartPoint = false, deprecatedWebkitEndPoint = false)
	background-color startColor
	if deprecatedWebkitStartPoint && deprecatedWebkitEndPoint
		background-image -webkit-gradient(linear, deprecatedWebkitStartPoint, deprecatedWebkitEndPoint, color-stop(startInterval, startColor), color-stop(endInterval, endColor))
	background-image -webkit-linear-gradient(startPoint, startColor startInterval, endColor endInterval)
	background-image -moz-linear-gradient(startPoint, startColor startInterval, endColor endInterval)
	background-image -ms-linear-gradient(startPoint, startColor startInterval, endColor endInterval)
	background-image -o-linear-gradient(startPoint, startColor startInterval, endColor endInterval)
	background-image linear-gradient(startPoint, startColor startInterval, endColor endInterval)
opacity(v)
	filter unquote('alpha(opacity=' + v*100 + ')')
	-moz-opacity v
	opacity v
/* DEFAULT */
.wc
	width 950px
	margin 0 auto
.dib
	display inline-block
	vertical-align top
	*display inline
	zoom 1
.dib_b
	@extends .dib
	vertical-align bottom
.dib_m
	@extends .dib
	vertical-align middle
.scw
	font-size 0
	white-space nowrap

.clr:before,
.clr:after
	content " "
	display table
.clr:after
	clear both
.clr
	*zoom 1
.bs 
	box-sizing(border-box)

body
	margin 0
	padding 0


.prezentator
	&-viewport
		position relative
		overflow hidden
		width 750px
		height 400px
		background-image unquote('url(data:image/png;base64,)')
		&:hover 
			& .prezentator-slides
				margin-bottom 0	 
			& .prezentator-arrow
				opacity(1)
	&-container
		position absolute
		width 100%
		height 100%
		overflow hidden
		&-front
		&-back 
			position absolute
			width 100%
			height 100%
			z-index 100
			text-align center
			& img 
				max-height 100%
				max-width 100%
				
		&-front 
			z-index 110
	&-slides
		position absolute
		width 100%
		height 20%
		overflow hidden
		bottom 0
		/*margin-bottom -15%*/
		margin-bottom -20%
		background-image unquote('url(data:image/png;base64,);')
		z-index 180
		font-size 0
		transition( all 200ms linear )
		&-wrapper 
			position relative
			height 100%
			white-space nowrap
			margin 0 10%
			width 80%
			overflow hidden
			&-container 
				width auto
				height 100%
				position absolute
		&-arrow 
			position absolute
			width 10%
			height 100%
			cursor pointer
			
			&-left 
				left 0
				&:after 
					
					left 50%
					top 50%	
					content " "
					position absolute
					border-style solid

					border-width 7.5px 10px 7.5px 0 
					border-color transparent white transparent transparent					
			&-right 
				right 0
				&:after
					left 50%
					top 50%
					content " "
					position absolute
					border-style solid

					border-width 7.5px 0 7.5px 10px
					border-color transparent transparent transparent white
	&-slide 
		@extends .dib
		@extends .bs
		position relative
		height 100%
		border 4px solid transparent
		border-top 0px
		cursor pointer
		max-height 100%
		font-size 0
		&.active 
			
			border 4px solid  #1e5981
			border-top 0px
			& .prezentator-slide-img
				opacity(1)
		&-img
			position relative
			/*opacity(0.6)*/
			transition(all 200ms linear)
			max-width 100%
			max-height 100%	
			height 100%	
			/*&:hover 
				opacity(1)			*/
			& img 
				max-width 100%
				max-height 100%




	&-arrow 
		position absolute
		width 15%
		background-color rgba(black, 0.75)
		height 100%
		z-index 150
		transition(all 400ms linear)
		opacity(0)
		cursor pointer
		&:after 
			color white 
			line-height 1em
			position absolute			
			font-size 72pt
		&-left 
			left 0
			&:after 
				content "«"
				top 35%
				left 20%
		&-right 
			right 0
			&:after 
				content "»"
				top 35%
				right 20%